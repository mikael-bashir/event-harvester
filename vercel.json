{
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/index.py"
        }
    ],
    "crons": [
        {
            "path": "/api/cron/poll-instagram",
            "schedule": "* * * * *"
        },
        {
            "path": "/api/health",
            "schedule": "0 0,12 * * *"
        }
    ]
}